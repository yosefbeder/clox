class Animal {
    init(type, name, birthYear) {
        this.type = type;
        this.name = name;
        this.birthYear = birthYear;
    }

    calcAge() {
        return 2022 - this.birthYear;
    }

    introduce() {
        var age = this.calcAge();

        return "I'm a ${this.type}, my name is ${this.name}, and I'm ${string(age)} year${age != 1? "s": ""} old!";
    }

    makeSound() {}
}

class Dog extends Animal {
    init(name, birthYear) {
        super.init;

        this.type = "Dog";
        this.name = name;
        this.birthYear = birthYear;
    }

    makeSound() {
        return "Woof... Woof... Woof...";
    }
}

var dog = Dog("Bobby", 2020);

print(dog.introduce());
print(dog.makeSound());
